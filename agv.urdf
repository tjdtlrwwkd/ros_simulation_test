    <link name="base_link">
        <pose>0 0 0.1 0 0 0</pose>
        <inertial>
            <mass value="5"/>
            <origin rpy="0 0 0" xyz="0 0 0.1"/>
            <inertia ixx="0.0395416666667" ixy="0" ixz="0" iyy="0.106208333333" iyz="0" izz="0.106208333333"/>
        </inertial>

        <visual>
            <geometry>
                <box size="0.398 0.46 0.05"/>
            </geometry>
            <origin xyz="-0.141 0.0 0.18"/>
            <material name="white">
                <color rgba="1.0 1.0 1.0 1.0"/>
            </material>
        </visual>

        <collision>
            <geometry>
                <box size="0.68 0.46 0.39"/>
            </geometry>
            <origin xyz="0.0 0.0 0.245"/>
        </collision>
    </link>

    <link name="top_link">
        <visual>
            <geometry>
                <box size="0.45 0.46 0.235"/>
            </geometry>
            <origin xyz="-0.115 0.0 0.32"/>
            <material name="white">
                <color rgba="1.0 1.0 1.0 1.0"/>
            </material>
        </visual>
    </link>

    <joint name="base_to_top_joint" type="fixed">
        <parent link="base_link"/>
        <child link="top_link"/>
        <origin xyz="0.0 0.0 0.0"/>
    </joint>

    <link name="top_front_link">
        <visual>
            <geometry>
                <cylinder radius="0.23" length="0.235"/>
            </geometry>
            <material name="white">
                <color rgba="1.0 1.0 1.0 1.0"/>
            </material>
        </visual>
    </link>

    <joint name="top_joint" type="fixed">
        <parent link="top_link"/>
        <child link="top_front_link"/>
        <origin xyz="0.11 0.0 0.32"/>
    </joint>
    
    <link name="bottom_link">
        <visual>
            <geometry>
                <box size="0.45 0.46 0.105"/>
            </geometry>
            <origin xyz="-0.115 0.0 0.1025"/>
            <material name="white">
                <color rgba="1.0 1.0 1.0 1.0"/>
            </material>
        </visual>
    </link>

    <joint name="base_to_bottom_joint" type="fixed">
        <parent link="base_link"/>
        <child link="bottom_link"/>
        <origin xyz="0.0 0.0 0.0"/>
    </joint>

    <link name="bottom_front_link">
        <visual>
            <geometry>
                <cylinder radius="0.23" length="0.105"/>
            </geometry>
            <material name="white">
                <color rgba="1.0 1.0 1.0 1.0"/>
            </material>
        </visual>
    </link>

    <joint name="bottom_joint" type="fixed">
        <parent link="bottom_link"/>
        <child link="bottom_front_link"/>
        <origin xyz="0.11 0.0 0.1025"/>
    </joint>

    <link name="left_wheel_case_link">
        <visual>
            <geometry>
                <box size="0.08 0.05 0.08"/>
            </geometry>
            <material name="white">
                <color rgba="1.0 1.0 1.0 1.0"/>
            </material>
        </visual>
    </link>

    <joint name="left_wheel_case_joint" type="fixed">
        <parent link="bottom_link"/>
        <child link="left_wheel_case_link"/>
        <origin xyz="0.05 0.15 0.154" rpy="0.0 0.7853 0.0"/>
    </joint>

    <link name="right_wheel_case_link">
        <visual>
            <geometry>
                <box size="0.08 0.05 0.08"/>
            </geometry>
            <material name="white">
                <color rgba="1.0 1.0 1.0 1.0"/>
            </material>
        </visual>
    </link>

    <joint name="right_wheel_case_joint" type="fixed">
        <parent link="bottom_link"/>
        <child link="right_wheel_case_link"/>
        <origin xyz="0.05 -0.15 0.154" rpy="0.0 0.7853 0.0"/>
    </joint>



    <link name="caster_front_link">
        <visual>
            <material name="blue">
                <color rgba="0.0 0.0 1.0 1.0"/>
            </material>
            <origin xyz="0.22 0.0 0.05" rpy="0.0 0.0 0.0"/>
            <geometry>
                <sphere radius="0.05"/>
                <!-- <sphere radius="0.05"/> -->
            </geometry>
        </visual>

        <collision name="caster_front_collision">
            <origin xyz="0.22 0.0 0.05" rpy="0.0 0.0 0.0"/>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>0</mu>
                        <mu2>0</mu2>
                        <slip1>1.0</slip1>
                        <slip2>1.0</slip2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </link>

    <joint name="caster_front_joint" type="fixed">
        <parent link="bottom_link"/>
        <child link="caster_front_link"/>
    </joint>

    <link name="caster_rear_left">
        <visual>
            <material name="blue">
                <color rgba="0.0 0.0 1.0 1.0"/>
            </material>
            <origin xyz="-0.22 0.12 0.05" rpy="0.0 0.0 0.0"/>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
        </visual>

        <collision name="caster_rear_left_collision">
            <origin xyz="0.22 0.0 0.05" rpy="0.0 0.0 0.0"/>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>0</mu>
                        <mu2>0</mu2>
                        <slip1>1.0</slip1>
                        <slip2>1.0</slip2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </link>

    <joint name="caster_rear_left_joint" type="fixed">
        <parent link="bottom_link"/>
        <child link="caster_rear_left"/>
    </joint>

    <link name="caster_rear_right">
        <visual>
            <material name="blue">
                <color rgba="0.0 0.0 1.0 1.0"/>
            </material>
            <origin xyz="-0.22 -0.12 0.05" rpy="0.0 0.0 0.0"/>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
        </visual>

        <collision name="caster_rear_right_collision">
            <origin xyz="0.22 0.0 0.05" rpy="0.0 0.0 0.0"/>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>0</mu>
                        <mu2>0</mu2>
                        <slip1>1.0</slip1>
                        <slip2>1.0</slip2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </link>

    <joint name="caster_rear_right_joint" type="fixed">
        <parent link="bottom_link"/>
        <child link="caster_rear_right"/>
    </joint>

    <!-- WHEEL -->
    <link name="link_right_wheel">
        <inertial>
            <mass value="0.2"/>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
        </inertial>

        <collision name="link_right_wheel_collision">
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.1"/>
            </geometry>
        </collision>

        <visual name="link_right_wheel_visual">
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.1"/>
            </geometry>
        </visual>
    </link>
    <!--  -->
    <joint name="joint_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="-0.0 -0.26 0.1"/>
        <child link="link_right_wheel"/>
        <parent link="base_link"/>
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>

    <link name="link_left_wheel">
        <inertial>
            <mass value="0.2"/>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
        </inertial>

        <collision name="link_left_wheel_collision">
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.1"/>
            </geometry>
        </collision>

        <visual name="link_left_wheel_visual">
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.1"/>
            </geometry>
        </visual>
    </link>
    
    <joint name="joint_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="-0.0 0.15 0.1"/>
        <child link="link_left_wheel"/>
        <parent link="bottom_link"/>
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>

